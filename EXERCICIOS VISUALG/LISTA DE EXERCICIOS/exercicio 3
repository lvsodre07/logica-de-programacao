Var
   // Seção de Declarações das variáveis
   valorA, valorB, c :inteiro

Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
escreval("digite valorA: ")
leia(valorA)

escreval("digite valorB")
leia(valorB)

se(valorA = valorB)entaoe
    c <- valorA + valorB
    escreval("o valor da soma é:", c)
senao
    c <- valorA * valorB
    escreval("o valor da multiplicacao é:", c)
fimse


